  <bpmn:process id="AP-001__Invoice_Processing_and_Approval" name="AP-001: Invoice Processing and Approval" isExecutable="false">
        <bpmn:startEvent id="StartEvent_1" name="Invoice Received"/>
    <bpmn:task id="step_1" name="Invoice Intake">
      <bpmn:documentation>Invoices are received via corporate email or supplier portal. System logs a new entry and assigns a unique tracking number.</bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="step_2" name="Initial Validation">
      <bpmn:documentation>Analyst verifies invoice completeness and legibility (vendor name, invoice number, date, total amount).</bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="step_3" name="Three-Way Match">
      <bpmn:documentation>System cross-references invoice against PO and GRN. Discrepancies trigger a hold and manual review by AP Supervisor.</bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="step_4" name="Threshold-Based Approval">
      <bpmn:documentation>Invoice routing based on amount: &lt;$5,000 to department manager; $5,000-$25,000 to senior manager; &gt;$25,000 to Director of Finance.</bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="step_5" name="Duplicate Check">
      <bpmn:documentation>System checks invoice ID, vendor details, and amount against historical payments to identify duplicate payments.</bpmn:documentation>
    </bpmn:task>
    <bpmn:task id="step_6" name="Payment Processing">
      <bpmn:documentation>Approved invoice is released to AP system for payment. Notification sent to vendor.</bpmn:documentation>
    </bpmn:task>
    <bpmn:exclusiveGateway id="gateway_1" name="Approval Routing"/>
    <bpmn:endEvent id="EndEvent_1" name="Payment Processed"/>
        <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="step_1"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="step_1" targetRef="step_2"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="step_2" targetRef="step_3"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="step_3" targetRef="step_4"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="step_4" targetRef="step_5"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="step_5" targetRef="step_6"/>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="step_6" targetRef="gateway_1"/>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="gateway_1" targetRef="EndEvent_1"/>
  </bpmn:process>
